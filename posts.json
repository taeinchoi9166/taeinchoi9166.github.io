---
permalink: /posts.json
---
{
  "posts": [
    {% for post in site.posts %}
      {
        "url":"{{ post.url }}",
        "title": "{{ post.title }}",
        "written_date": "{{ post.date }}",
        "categories": {{ post.categories | split: "\" }},
        "content": "{{ post.content | strip_html | replace: """ , ""  | replace: "'", ""}}"
      }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}
